{
  "name": "example-app",
  "version": "2.0.0-react",
  "private": true,
  "description": "An example cashier application built with the axleLLM engine.",
  "author": "Xzdes",
  "main": "node_modules/axle-llm/main.js",
"scripts": {
    "// --- BUILD SCRIPTS ---": "",
    "build": "node ../axle-llm/core/build.js",
    "watch": "node ../axle-llm/core/build.js --watch",
    "build:client": "node ../axle-llm/core/build-client.js",
    "watch:client": "node ../axle-llm/core/build-client.js --watch",
    "// --- LIFECYCLE SCRIPTS ---": "",
    "dev": "axle-cli dev",
    "start": "npm run build && npm run build:client && axle-cli start",
    "package": "axle-cli package",
    "seed": "node seed.js"
  },
  "dependencies": {
    "axle-llm": "file:../axle-llm",
    "bcrypt": "6.0.0",
    "wise-json-db": "6.0.2"
  },
  "devDependencies": {
    "electron": "37.2.5",
    "electron-builder": "26.0.12"
  },
  "build": {
    "appId": "com.axlellm.example-app",
    "productName": "Атомарная Касса (React)",
    "copyright": "Copyright © 2025 Xzdes",
    "asar": true,
    "files": [
      "app/",
      "manifest/",
      "manifest.js",
      "axle-db-data/",
      ".axle-build/",
      "public/",
      "!dist/"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}