
{
  "name": "example-app",
  "version": "1.1.0",
  "private": true,
  "description": "An example cashier application built with the axleLLM engine.",
  "author": "Xzdes",
  "main": "node_modules/axle-llm/main.js",
  "scripts": {
    "dev": "axle-cli dev",
    "start": "axle-cli start",
    "package": "axle-cli package",
    "seed": "node seed.js"
  },
    "dependencies": {
    "axle-llm": "2.1.1",
    "bcrypt": "6.0.0",
    "wise-json-db": "6.0.2"
    },
    "devDependencies": {
    "electron": "37.2.4",
    "electron-builder": "26.0.19"
  },
  "build": {
    "appId": "com.axlellm.example-app",
    "productName": "Атомарная Касса",
    "copyright": "Copyright © 2025 Xzdes",
    "asar": true,
    "files": [
      "!dist/",
      "app/",
      "manifest/",
      "manifest.js",
      "axle-db-data/"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}



